		{% block content %}
		  
		  
		{% endblock %}



	 {% block javascripts %}
		<script src="{{ asset('js/google-closure/closure/goog/base.js')}}"></script>	   

		{% javascripts 
		    '@NasajonCalendarioBundle/Resources/public/js/jquery/jquery-1.11.1.min.js'
		    '@NasajonCalendarioBundle/Resources/public/js/jquery/jquery.price_format.1.8.min.js'
		    '@NasajonCalendarioBundle/Resources/public/js/jquery/jquery.price_format.1.8.min.js'
		    '@NasajonCalendarioBundle/Resources/public/js/pages/scripts.js'		    
		 %}
		<script src="{{ asset_url }}"></script>
		{% endjavascripts %}
		<script src="{{ asset('js/bootstrap.js') }}"></script>	   

	{% endblock %}
